{
  "_comment": "General training hyperparameters shared across all models. Model-specific sections override where needed.",
  "general": {
    "num_epochs": 500,
    "lr": 0.0001,
    "lr_decay_factor": 0.5,
    "lr_decay_step": 100,
    "optimizer": "adam",
    "loss": "l1",
    "batch_size": 512,
    "PRN_size": 32,
